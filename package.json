{
  "name": "video-mindmap",
  "displayName": "Video Summary",
  "version": "0.0.1",
  "description": "Summary video, or convert video to mindmap",
  "author": "Dexter",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "mind-elixir": "5.0.0-beta.34",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "https://api.bilibili.com/*",
      "https://www.bilibili.com/*",
      "https://www.youtube.com/*",
      "https://youtube.com/*"
    ],
    "permissions": [
      "activeTab",
      "storage"
    ],
    "options_page": "options.html",
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    }
  }
}
